<div id="list-set-<%=list_set.id%>"
      class="bg-white text-black p-2 border border-green-100 rounded"
      data-controller="update"
      data-update-name-value="ListSet"
      data-update-id-value="<%= list_set.id%>" >
  <div><%=title%></div>
  <div id="list-set-contents-<%= list_set.id %>">
    <%= render partial: 'list_sets/items', locals: { list_set: list_set } %>
  </div>
  <div>
    <%= link_to(t(:new_list), current_new_list_path, method: :post) %>
  </div>
</div>
